{
  "name": "express_template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/server.ts",
    "start:debug": "ts-node-dev --transpile-only --respawn --inspect=4321 --project tsconfig.json src/server.ts",
    "dev": "nodemon --exec ts-node src/server.ts",
    "test": "jest --watchAll",
    "test:coverage": "jest --watchAll --coverage",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "prettier-check": "prettier --check --no-error-on-unmatched-pattern \"src/**/*.ts\"",
    "prettier-write": "prettier --write --no-error-on-unmatched-pattern \"src/**/*.ts\"",
    "migrate": "npx prisma migrate deploy"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.0.0",
    "@types/cors": "^2.8.17",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-validator": "^7.2.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prisma": "^6.0.0",
    "ts-node-dev": "^2.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^6.6.0",
    "prettier": "^3.4.1",
    "typescript": "^5.7.2"
  }
}
